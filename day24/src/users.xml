<?xml version="1.0" encoding="UTF-8" ?>
<!--文档声明必须放在第一行-->
<users>
    <user>
        <name>001</name>
        <age>12</age>
        <sex>男</sex>
    </user>
    <user>
        <name>002</name>
        <age>12</age>
        <sex>男</sex>
        <code>
            <!--有特殊字符时可以使用CDATA区域-->
            <![CDATA[
               if(a<10){}
            ]]>
        </code>
    </user>
    <user id="111">
        <name>003</name>
        <age>12</age>
        <sex>男</sex>
        <code>
            <!--转义字符-->
            if(a &lt; 10){}
        </code>
    </user>
    <user>
        <name class="nnn">004</name>
        <age>12</age>
        <sex>男</sex>
    </user>
</users>